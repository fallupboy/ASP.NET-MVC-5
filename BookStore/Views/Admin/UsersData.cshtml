
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>All Users</h3>
<table class="table">
    <tr class="tr_main">
        <td><p>ID</p></td>
        <td><p>First Name</p></td>
        <td><p>Second Name</p></td>
        <td><p>Email</p></td>
    </tr>
    @foreach (var b in ViewBag.Users)
    {
        <tr class="tr_content">
            <td><p>@b.Id</p></td>
            <td><p>@b.FirstName</p></td>
            <td><p>@b.LastName</p></td>
            <td><p>@b.Email</p></td>
        </tr>
    }
</table>
<a href="@Url.Action("Index", "Home")" class="btn btn-info">Back To Store</a>
